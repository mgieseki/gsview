gsview_accel ACCELERATORS
BEGIN
        "O", IDM_OPEN
        "o", IDM_OPEN
        "C", IDM_CLOSE
        "c", IDM_CLOSE
        "N", IDM_NEXT
        "n", IDM_NEXT
        "+", IDM_NEXT
        " ", IDM_NEXTHOME
        VK_SPACE, IDM_NEXTHOME, VIRTKEY
        "V", IDM_PREV
        "v", IDM_PREV
        "-", IDM_PREV
        VK_BACK, IDM_PREVHOME, VIRTKEY
        VK_BACK, IDM_PREVHOME, VIRTKEY, SHIFT
        "G", IDM_GOTO
        "g", IDM_GOTO
        "B", IDM_GOBACK
        "b", IDM_GOBACK
        "I", IDM_INFO
        "i", IDM_INFO
        "R", IDM_REDISPLAY
        "r", IDM_REDISPLAY
        "S", IDM_SELECT
        "s", IDM_SELECT
        "A", IDM_SAVEAS
        "a", IDM_SAVEAS
        "P", IDM_PRINT
        "p", IDM_PRINT
        "F", IDM_CONVERTFILE
        "f", IDM_CONVERTFILE
#ifdef OLD
        VK_F, IDM_TEXTFIND, VIRTKEY, CONTROL
#else
        "^F", IDM_TEXTFIND
#endif
        VK_F3, IDM_TEXTFINDNEXT, VIRTKEY
        "E", IDM_EXTRACT
        "e", IDM_EXTRACT
        "M", IDM_GSMESS
        "m", IDM_GSMESS
        "<", IDM_MAGMINUS
        ">", IDM_MAGPLUS
        ",", IDM_MAGMINUS
        ".", IDM_MAGPLUS
        VK_F1, IDM_HELPCONTENT, VIRTKEY
        "^C", IDM_COPYCLIP
        VK_INSERT, IDM_COPYCLIP, VIRTKEY, CONTROL
        VK_F4, IDM_FULLSCREEN, VIRTKEY
        VK_F5, IDM_REDISPLAY, VIRTKEY
        VK_F6, IDM_FITWIN, VIRTKEY
END



