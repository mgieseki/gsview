gsview_menu MENU
BEGIN
	POPUP "&Αρχείο"
	BEGIN
		MENUITEM "’ν&οιγμα...\tΟ", IDM_OPEN
		MENUITEM "Επι&λογή αρχείου...\tΛ", IDM_SELECT
		MENUITEM "Αποθήκευση &ως...\tΩ", IDM_SAVEAS
		MENUITEM "&Κλείσιμο...\tΚ",  IDM_CLOSE
		MENUITEM "&Πληροφορίες...\tΠ",  IDM_INFO
		MENUITEM SEPARATOR
		MENUITEM "Μετατ&ροπή...\tΡ", IDM_CONVERTFILE
		MENUITEM "Εξα&γωγή...\tΓ", IDM_EXTRACT
		MENUITEM "PS &σε EPS", IDM_PSTOEPS
		MENUITEM SEPARATOR
		MENUITEM "&Εκτύπωση...\tE", IDM_PRINT
		MENUITEM  "Εκτύπωση &του αρχείου...", IDM_SPOOL
		MENUITEM  SEPARATOR
		MENUITEM  "Παρουσίαση &μηνυμάτων\tΜ", IDM_GSMESS 
