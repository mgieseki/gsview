		END
		POPUP "Προειδοποιητικά μηνύματα δομής DSC"
		BEGIN
			MENUITEM "Απενεργοποιημένα", IDM_DSC_OFF
			MENUITEM "Σφάλματα", IDM_DSC_ERROR
			MENUITEM "Προειδοποιήσεις", IDM_DSC_WARN
			MENUITEM "Όλα", IDM_DSC_INFO
		END
