		MENUITEM "Απομνημόνευση τελευταίου &καταλόγου", IDM_SAVEDIR
		MENUITEM "Εμφάνιση της γραμμής &εργαλείων", IDM_BUTTONSHOW
		MENUITEM "Προσαρμογή του &παράθυρου στη σελίδα", IDM_FITPAGE
		MENUITEM "Α&υτόματη επανεμφάνιση", IDM_AUTOREDISPLAY
		MENUITEM "Περικ&οπή EPS", IDM_EPSFCLIP
		MENUITEM "Π&ροειδοποιήσεις EPS", IDM_EPSFWARN
		MENUITEM "Παρά&βλεψη σχολίων δομής DSC", IDM_IGNOREDSC
		MENUITEM "Εμ&φάνιση ορίων πλαισίου", IDM_SHOWBBOX
	END
	POPUP "Π&ροβολή"
	BEGIN
		MENUITEM "&Επόμενη σελίδα\t+", IDM_NEXT
		MENUITEM "&Προηγούμενη σελίδα\t-", IDM_PREV
		MENUITEM "&Μετάβαση στη σελίδα...\tΜ", IDM_GOTO
		MENUITEM "Πί&σω\tΣ", IDM_GOBACK
		MENUITEM "Εμπ&ρός\tΡ", IDM_GOFWD
		MENUITEM "&Ανανέωση οθόνης\tΑ", IDM_REDISPLAY
		MENUITEM SEPARATOR
		MENUITEM "Αρχή επόμενης σελίδας\tΔιάστημα", IDM_NEXTHOME
		MENUITEM "Αρχή προηγούμενης σελίδας\tBackspace", IDM_PREVHOME
		MENUITEM SEPARATOR
		MENUITEM "Προβολή πλήρους &οθόνης\tF4", IDM_FULLSCREEN
		MENUITEM "Προσαρμογή στο παρά&θυρο\tF6", IDM_FITWIN
	END
	POPUP "&Προσανατολισμός"
	BEGIN
		MENUITEM "&Αυτόματος", IDM_AUTOORIENT
		MENUITEM SEPARATOR
		MENUITEM "&Κατακόρυφος", IDM_PORTRAIT
		MENUITEM "&Οριζόντιος", IDM_LANDSCAPE
		MENUITEM "Ανε&στραμμένος", IDM_UPSIDEDOWN
		MENUITEM "&Πανοραμικός", IDM_SEASCAPE
		MENUITEM SEPARATOR
		MENUITEM "Οριζόντια &μετάθεση", IDM_SWAPLANDSCAPE
	END
	POPUP "&Μέσα"
	BEGIN
		MENUITEM "&Ρυθμίσεις οθόνης...", IDM_DISPLAYSETTINGS
		MENUITEM SEPARATOR
		MENUITEM "&Περιστροφή μέσου", IDM_MEDIAROTATE
		MENUITEM SEPARATOR
		MENUITEM "11x17 ίντσες", IDM_11x17
		MENUITEM "A3", IDM_A3
		MENUITEM "A4", IDM_A4
		MENUITEM "A5", IDM_A5
		MENUITEM "B4", IDM_B4
		MENUITEM "B5", IDM_B5
		MENUITEM "Ledger", IDM_LEDGER
		MENUITEM "Legal", IDM_LEGAL
		MENUITEM "Letter", IDM_LETTER
		MENUITEM "Note", IDM_NOTE
		MENUITEM "&Καθορισμένο από το χρήστη...", IDM_USERSIZE
	END
	POPUP "&Βοήθεια"
	BEGIN
		MENUITEM "Π&εριεχόμενα", IDM_HELPCONTENT
		MENUITEM "&Αναζήτηση βοήθειας για...", IDM_HELPSEARCH
		MENUITEM "Βοήθεια για τα π&λήκτρα", IDM_HELPKEYS
		MENUITEM SEPARATOR
		MENUITEM "&Δήλωση...", IDM_REGISTER
		MENUITEM "&Πληροφορίες...", IDM_ABOUT
	END
END

