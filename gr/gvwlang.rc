		BEGIN
			MENUITEM "&English", IDM_LANGEN
			/* Remaining languages are added at run time */
		END
		POPUP "PS σε &κείμενο"
		BEGIN
			MENUITEM "Απενεργοποιημένη", IDM_PSTOTEXTDIS
			MENUITEM "Κανονική", IDM_PSTOTEXTNORM
